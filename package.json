  "main": "index.js",
  "module": "index.m.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dwightjack/storybook-addon-storyout.git"
  },
  "bugs": "https://github.com/dwightjack/storybook-addon-storyout/issues",
  "scripts": {
    "build": "node ./scripts/build",
    "lint:eslint": "eslint \"src/**/*.ts\"",
    "lint:tsc": " tsc --noEmit -p .",
